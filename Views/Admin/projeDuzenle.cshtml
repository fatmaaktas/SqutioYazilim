@model SqutioYzlm.Models._site
@{
    ViewBag.Title = "projeDuzenle";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}



<!-- Bootstrap 4.1-->
<link rel="stylesheet" href="/Content/admincss/bootstrap.min.css">
<!--alerts CSS -->
<link href="/Content/admincss/sweetalert.css" rel="stylesheet" type="text/css">

<div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <!-- Main content -->
    <section class="content">
        <div class="container">

            <div class="row">
                <div class="col-12">
                    <!-- /.box -->
                    <div class="box">
                        <div class="box-header with-border bg-light">
                            <h4 class="box-title">@Model.m_proje.proje_adi</h4>
                        </div>
                        <!-- /.box-header -->
                        <div class="box-body wizard-content">


                            <!-- Step 1 -->

                            <form method="post" action="/Admin/projeDuzenle" name="frm_mm">
                                <input type="hidden" name="id" value="@Model.m_proje.id" />

                                <section>
                                    <div class="row" style="border:1px solid #e1e1e1;padding:20px;">
                                        <div class="col-md-12">
                                            <div class="form-group">

                                                <label>Proje Adı : </label>
                                                <input type="text" name="proje_adi" class="form-control" value="@Model.m_proje.proje_adi" />
                                            </div>
                                        </div>
                                        <div class="col-md-12">
                                            <div class="form-group">

                                                <label>Konusu : </label>
                                                <input name="proje_konu" class="form-control" value="@Model.m_proje.proje_konu" />
                                            </div>
                                        </div>
                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <input type="submit" value="Kaydet" class="btn btn-default btn-primary" />
                                            </div>
                                        </div>


                                    </div>
                                </section>
                            </form>

                            <!-- Step 2 -->

                        </div>
                        <!-- /.box-body-->
                    </div>
                    <!-- /.box -->

                </div>
            </div>
        </div>
    </section>
    <!-- /.content -->
</div>

<!-- /.content-wrapper -->
<!-- Bootstrap 4.1-->
<script src="/Content/adminjs/bootstrap.min.js"></script>

<!-- SlimScroll -->
<!-- steps  -->
<script src="/Content/adminjs/jquery.steps.js"></script>

<!-- validate  -->
<script src="/Content/adminjs/jquery.validate.min.js"></script>

<!-- Sweet-Alert  -->
<script src="/Content/adminjs/sweetalert.min.js"></script>

<!-- wizard  -->
<script src="/Content/adminjs/steps.js"></script>
